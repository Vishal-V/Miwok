<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.miwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/headphone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/headphone"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/numbers" android:name=".NumbersActivity" />
        <activity android:label="@string/family" android:name=".FamilyMembersActivity" />
        <activity android:label="@string/colors" android:name=".ColorsActivity" />
        <activity android:label="@string/phrases" android:name=".PhrasesActivity"></activity>
    </application>

</manifest>